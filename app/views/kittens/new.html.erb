<h1>Create a new kitten!</h1>
<%= form_with model: @kitten do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :age %>
  <%= f.number_field :age %>
  <%= f.label :cuteness %>
  <%= f.number_field :cuteness, in: 1..10 %>
  <%= f.label :softness %>
  <%= f.number_field :softness, in: 1..10 %>
  <%= f.submit %>
<% end %>